{
  "root": true,
  "rules": {
    "no-underscore-dangle": [
      "error", {
        "allow": [ "_id" ]
      }
    ],
    "no-const-assign": [
      "error"
    ],
    "consistent-return": "off",
    "import/no-extraneous-dependencies": "off",
    "@typescript-eslint/no-var-requires": "off",
    "no-console": "off",
    "max-len": "off",
    "no-useless-escape": "off",
    "max-classes-per-file": "off"
  },
  "extends": "airbnb-base"
}